{
	"manifest_version": 2,
	"name": "MyGS Custom Layout",
	"description": "Customize your MyGeorgiaSouthern layout by dragging around the tiles",
	"version": "1.0.1",
	"content_scripts": [ {
		"matches": [ "https://my.georgiasouthern.edu/portal/portal.php*" ],
		"js" : [
			"lib/jquery-3.2.1.js",
			"lib/jquery-ui.js",
			"main/main.js"
		],
		"css": [
			"main/main.css"
		],
		"run_at": "document_end"
	} ],
	"background": {
		"persistent": false,
		"scripts": ["main/background.js"]
	},
	"permissions": [
		"https://my.georgiasouthern.edu/*",
		"storage",
		"contextMenus"
	],
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png"
	}
}